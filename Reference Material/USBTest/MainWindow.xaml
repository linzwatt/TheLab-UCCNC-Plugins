        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button Width="100" Content="Create 1" Click="Button_Click_1" Margin="2" />
            <Button Width="100" Content="Send 1" Click="Button_Click_2" Margin="2" />
            <Button Name="button1" Width="100" Content="Create 2" Click="Button_Click_3" Margin="2" />
